@model Dictionary<string,int>

<table class="table">
    <tr>
        <th>
            Username
        </th>
        <th>
            Number of Tasks
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.Key
        </td>
        <td>
            @item.Value
        </td>
    </tr>
}

</table>
