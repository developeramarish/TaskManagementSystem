@model Dictionary<string, int>

<table class="table">
    <tr>
        <th>
            Project name
        </th>
        <th>
            Tasks left
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Key
            </td>
            <td>
                @item.Value
            </td>
        </tr>
    }

</table>
